{
    "version": 2,
    "builds": [
        {
        "src": "/API/server.js",
        "use": "@vercel/node"
        }
    ],
    "buildCommand": "cd client && npm install && npm run build",
    "outputDirectory": "client/dist",
    "framework": "vite",
    "rewrites": [
      {
        "source": "/API(.*)",
        "destination": "/API/server.js"
      }
    ]
}